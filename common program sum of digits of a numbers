import java.io.*;
import java.util.*;
class Sumn
{
    public static void main(String[] args)
    {
        Scanner z=new Scanner(System.in);
        long n,s,r;
        System.out.println("enter n value");
        n=z.nextLong();
        s=0;
        while(n>0)
        {
            r=n%10;
            s=s+r;
            n=n/10;
        }
        System.out.println("sum of digits="+s);
    }
}
