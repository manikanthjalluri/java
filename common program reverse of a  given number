import java.io.*;
import java.util.*;
class  reverse
{
    public static void main(String[]args)
    {
        Scanner z=new Scanner(System.in);
        int n,rev=0,r;
        System.out.println("enter n value");
        n=z.nextInt();
        
        while(n>0)
        {
            r=n%10;
            rev=rev*10+r;
            n=n/10;
        }
        System.out.println(rev);
    }
}




