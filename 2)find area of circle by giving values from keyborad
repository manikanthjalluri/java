import java.io.*;
import java.util.*;
class Circle

{
    public static void main(String[]args)
    {
        double r;
        double area;
        Scanner s =new Scanner(System.in);
        System.out.println("enter r value");
        r=s.nextDouble();
        area=3.14*r*r;
        System.out.println("area"+area);
        
    }
}

