import java.io.*;
import java.util.*;
class Lists
{
    public static void main(String[] args)
    {
        String[] courses={"c","c++","python","java","c#","html"};
        int i;
        System.out.println("computer courses");
        for(i=0;i<courses.length;i++)
        {
            System.out.println(courses[i]+" ");
        }
        System.out.println("computer coursrs are");
        for(String c:courses)
        {
            System.out.println(c+" ");
        }
    }
}
