import java.io.*;
import java.util.*;
class Factorial
{
    public static void main(String[] args)
    {
        Scanner z=new Scanner(System.in);
        long n,fact=1;
        System.out.println("enter any number");
        n=z.nextLong();
        while(n>0)
        {
            fact=fact*n;
            n--;
        }
        System.out.println("factorial of given number="+fact);
    }
}
